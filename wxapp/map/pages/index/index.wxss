.container {
  margin: 0;
  padding: 0;
}
.tips {
  width: 700rpx;
  height: 100rpx;
  position: absolute;
  text-align: center;
  z-index: 1;
  background-color: #fff;
  border-radius: 24rpx;
  top: 40rpx;
}
.tip1 {
  font-size: 26rpx;
  position: absolute;
  display: inline-block;
  left: 120rpx;
  top: 30rpx;
}
.tip2 {
  font-size: 26rpx;
  position: absolute;
  display: inline-block;
  top: 30rpx;
  color: orange;
}
.worning {
  width: 700rpx;
  height: 70rpx;
  box-sizing: border-box;
  padding: 14rpx 90rpx;
  position: absolute;
  z-index: 1;
  background-color: #fff;
  border-radius: 24rpx;
  top: 900rpx;
  font-size: 28rpx;
  color: #6f7071;
}

.fuc {
  width: 80rpx;
  height: 200rpx;
  position: absolute;
  right: 36rpx;
  top: 400rpx;
  z-index: 1;
  background-color: #fff;
  border-radius: 20rpx;
}
.fuc-findbike {
  height: 90rpx;
  position: absolute;
  top: 16rpx;
  right: 10px;
  border-bottom: 4rpx solid #f2f2f2 ;
}
.lightning {
  width: 60rpx;
  height: 70rpx;
}
.fuc-stopbike {
  height: 100rpx;
  font-size: 60rpx;
  color: #a2a2a2;
  position: absolute;
  top: 105rpx;
  right: 20rpx;
}
.ride {
  position: absolute;
  background-color: #26292f;
  color: #fff;
  font-size: 40rpx;
  height: 113.9rpx;
  width: 707rpx;
  bottom: 106.9rpx;
  text-align: center;
  z-index: 1;
  border-radius: 20rpx;
}
.scan {
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  display: inline-block;
  left: 200rpx;
  top: 30rpx;
}
.rental {
  position: absolute;
  height: 60rpx;
  display: inline-block;
  left: 290rpx;
  top: 30rpx;
}
.location {
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  top: 700rpx;
  right: 36rpx;
  background-color: #fff;
  z-index: 1;
}
.location image,
.repair image {
  width: 52rpx;
  height: 52rpx;
  margin-top: 15rpx;
  margin-left: 15rpx;
}
.repair {
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  top: 810rpx;
  right: 36rpx;
  background-color: #fff;
  z-index: 1;
}
